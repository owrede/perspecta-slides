{
  "name": "Zurich",
  "version": "1.0.0",
  "author": "Perspecta Slides",
  "description": "Minimal Swiss design with clean typography",
  "fonts": {
    "title": {
      "name": "Helvetica Neue",
      "css": "Helvetica Neue, Helvetica, Arial, sans-serif"
    },
    "body": {
      "name": "Helvetica Neue",
      "css": "Helvetica Neue, Helvetica, Arial, sans-serif"
    }
  },
  "presets": {
    "light": {
      "background": "#ffffff",
      "titleText": "#000000",
      "bodyText": "#333333",
      "frame": "#f5f5f5",
      "accent1": "#000000",
      "accent2": "#43aa8b",
      "accent3": "#f9c74f",
      "accent4": "#90be6d",
      "accent5": "#f8961e",
      "accent6": "#577590"
    },
    "dark": {
      "background": "#000000",
      "titleText": "#ffffff",
      "bodyText": "#cccccc",
      "frame": "#1a1a1a",
      "accent1": "#ffffff",
      "accent2": "#43aa8b",
      "accent3": "#f9c74f",
      "accent4": "#90be6d",
      "accent5": "#f8961e",
      "accent6": "#577590"
    }
  },
  "layouts": {
    "cover": {
      "appearance": "dark"
    },
    "section": {
      "appearance": "dark"
    }
  }
