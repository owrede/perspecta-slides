{
    "name": "Vancouver",
    "version": "2.0.0",
    "author": "Perspecta",
    "description": "Natural theme inspired by Pacific Northwest landscapes",
    "fonts": {
        "title": {
            "name": "Montserrat",
            "css": "Montserrat, system-ui, sans-serif"
        },
        "body": {
            "name": "Montserrat",
            "css": "Montserrat, system-ui, sans-serif"
        }
    },
    "cssClasses": "variable-size-headings",
    "presets": {
        "light": {
            "text": {
                "h1": [
                    "#2d3e2f"
                ],
                "h2": [
                    "#2d3e2f"
                ],
                "h3": [
                    "#3d4e3f"
                ],
                "h4": [
                    "#3d4e3f"
                ],
                "body": "#4a5a4c",
                "header": "#7a8a7c",
                "footer": "#7a8a7c"
            },
            "backgrounds": {
                "general": {
                    "type": "dynamic",
                    "colors": [
                        "#cfcac7",
                        "#d7c7c4",
                        "#bac8c1",
                        "#c9d1bf",
                        "#eadfce",
                        "#e2e2e2"
                    ]
                },
                "cover": {
                    "type": "gradient",
                    "colors": [
                        "#2d3e2f",
                        "#4a5a4c"
                    ]
                },
                "title": {
                    "type": "solid",
                    "color": "#f5f5f0"
                },
                "section": {
                    "type": "solid",
                    "color": "#2d3e2f"
                }
            },
            "accents": [
                "#a7987c",
                "#2d3e2f",
                "#739382",
                "#8ea079",
                "#a6916a",
                "#6b8e6b"
            ]
        },
        "dark": {
            "text": {
                "h1": [
                    "#f5f5f0"
                ],
                "h2": [
                    "#f5f5f0"
                ],
                "h3": [
                    "#e5e5e0"
                ],
                "h4": [
                    "#e5e5e0"
                ],
                "body": "#d5d5d0",
                "header": "#a5a5a0",
                "footer": "#a5a5a0"
            },
            "backgrounds": {
                "general": {
                    "type": "dynamic",
                    "colors": [
                        "#4d4036",
                        "#72483e",
                        "#739382",
                        "#8ea079",
                        "#a6916a",
                        "#848484"
                    ]
                },
                "cover": {
                    "type": "gradient",
                    "colors": [
                        "#1a261c",
                        "#2d3e2f"
                    ]
                },
                "title": {
                    "type": "solid",
                    "color": "#1a261c"
                },
                "section": {
                    "type": "solid",
                    "color": "#a7987c"
                }
            },
            "accents": [
                "#a7987c",
                "#f5f5f0",
                "#739382",
                "#8ea079",
                "#a6916a",
                "#6b8e6b"
            ]
        }
    }
}