{
    "name": "Garamond",
    "version": "2.0.0",
    "author": "Perspecta",
    "description": "Elegant serif typography with classic proportions",
    "fonts": {
        "title": {
            "name": "EB Garamond",
            "css": "EB Garamond, Garamond, Georgia, serif"
        },
        "body": {
            "name": "EB Garamond",
            "css": "EB Garamond, Garamond, Georgia, serif"
        }
    },
    "cssClasses": "variable-size-headings serif-theme",
    "presets": {
        "light": {
            "text": {
                "h1": [
                    "#1a1a1a"
                ],
                "h2": [
                    "#1a1a1a"
                ],
                "h3": [
                    "#2d2d2d"
                ],
                "h4": [
                    "#2d2d2d"
                ],
                "body": "#333333",
                "header": "#666666",
                "footer": "#666666"
            },
            "backgrounds": {
                "general": {
                    "type": "solid",
                    "color": "#fdfbf7"
                },
                "cover": {
                    "type": "solid",
                    "color": "#fdfbf7"
                },
                "title": {
                    "type": "solid",
                    "color": "#fdfbf7"
                },
                "section": {
                    "type": "solid",
                    "color": "#1a1a1a"
                }
            },
            "accents": [
                "#8b0000",
                "#1a1a1a",
                "#c9a87c",
                "#2f4f4f",
                "#daa520",
                "#4a4a4a"
            ]
        },
        "dark": {
            "text": {
                "h1": [
                    "#f5f0e6"
                ],
                "h2": [
                    "#f5f0e6"
                ],
                "h3": [
                    "#e5ddd4"
                ],
                "h4": [
                    "#e5ddd4"
                ],
                "body": "#d5cdc4",
                "header": "#a09891",
                "footer": "#a09891"
            },
            "backgrounds": {
                "general": {
                    "type": "solid",
                    "color": "#1a1a1a"
                },
                "cover": {
                    "type": "solid",
                    "color": "#0d0d0d"
                },
                "title": {
                    "type": "solid",
                    "color": "#1a1a1a"
                },
                "section": {
                    "type": "solid",
                    "color": "#8b0000"
                }
            },
            "accents": [
                "#c9a87c",
                "#f5f0e6",
                "#8b0000",
                "#2f4f4f",
                "#daa520",
                "#a09891"
            ]
        }
    }
}