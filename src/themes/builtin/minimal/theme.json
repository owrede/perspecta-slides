{
  "name": "Minimal",
  "version": "2.0.0",
  "author": "Perspecta",
  "description": "Clean, distraction-free design with generous whitespace",
  "fonts": {
    "title": {
      "name": "Inter",
      "css": "Inter, system-ui, sans-serif"
    },
    "body": {
      "name": "Inter",
      "css": "Inter, system-ui, sans-serif"
    }
  },
  "cssClasses": "variable-size-headings",
  "presets": {
    "light": {
      "text": {
        "h1": [
          "#111111"
        ],
        "h2": [
          "#111111"
        ],
        "h3": [
          "#333333"
        ],
        "h4": [
          "#333333"
        ],
        "body": "#444444",
        "header": "#888888",
        "footer": "#888888"
      },
      "backgrounds": {
        "general": {
          "type": "solid",
          "color": "#fafafa"
        },
        "cover": {
          "type": "solid",
          "color": "#fafafa"
        },
        "title": {
          "type": "solid",
          "color": "#fafafa"
        },
        "section": {
          "type": "solid",
          "color": "#111111"
        }
      },
      "accents": [
        "#111111",
        "#666666",
        "#999999",
        "#cccccc",
        "#e63946",
        "#2a9d8f"
      ]
    },
    "dark": {
      "text": {
        "h1": [
          "#ffffff"
        ],
        "h2": [
          "#ffffff"
        ],
        "h3": [
          "#e0e0e0"
        ],
        "h4": [
          "#e0e0e0"
        ],
        "body": "#cccccc",
        "header": "#888888",
        "footer": "#888888"
      },
      "backgrounds": {
        "general": {
          "type": "solid",
          "color": "#111111"
        },
        "cover": {
          "type": "solid",
          "color": "#111111"
        },
        "title": {
          "type": "solid",
          "color": "#111111"
        },
        "section": {
          "type": "solid",
          "color": "#ffffff"
        }
      },
      "accents": [
        "#ffffff",
        "#999999",
        "#666666",
        "#333333",
        "#e63946",
        "#2a9d8f"
      ]
    }
  }
}