{
    "name": "New York",
    "version": "2.0.0",
    "author": "Perspecta",
    "description": "Bold, urban design inspired by New York City",
    "fonts": {
        "title": {
            "name": "Oswald",
            "css": "Oswald, Impact, sans-serif"
        },
        "body": {
            "name": "Open Sans",
            "css": "Open Sans, system-ui, sans-serif"
        }
    },
    "cssClasses": "variable-size-headings bold-headings",
    "presets": {
        "light": {
            "text": {
                "h1": [
                    "#0a0a0a"
                ],
                "h2": [
                    "#0a0a0a"
                ],
                "h3": [
                    "#1a1a1a"
                ],
                "h4": [
                    "#1a1a1a"
                ],
                "body": "#333333",
                "header": "#666666",
                "footer": "#666666"
            },
            "backgrounds": {
                "general": {
                    "type": "solid",
                    "color": "#ffffff"
                },
                "cover": {
                    "type": "solid",
                    "color": "#f5f5f5"
                },
                "title": {
                    "type": "solid",
                    "color": "#ffffff"
                },
                "section": {
                    "type": "solid",
                    "color": "#0a0a0a"
                }
            },
            "accents": [
                "#ffd700",
                "#0a0a0a",
                "#ff4500",
                "#1e90ff",
                "#32cd32",
                "#9400d3"
            ]
        },
        "dark": {
            "text": {
                "h1": [
                    "#ffffff"
                ],
                "h2": [
                    "#ffffff"
                ],
                "h3": [
                    "#e0e0e0"
                ],
                "h4": [
                    "#e0e0e0"
                ],
                "body": "#cccccc",
                "header": "#888888",
                "footer": "#888888"
            },
            "backgrounds": {
                "general": {
                    "type": "solid",
                    "color": "#0a0a0a"
                },
                "cover": {
                    "type": "solid",
                    "color": "#000000"
                },
                "title": {
                    "type": "solid",
                    "color": "#0a0a0a"
                },
                "section": {
                    "type": "solid",
                    "color": "#ffd700"
                }
            },
            "accents": [
                "#ffd700",
                "#ffffff",
                "#ff4500",
                "#1e90ff",
                "#32cd32",
                "#9400d3"
            ]
        }
    }
}