{
    "name": "Paris",
    "version": "2.0.0",
    "author": "Perspecta",
    "description": "Romantic, elegant theme with soft French aesthetics",
    "fonts": {
        "title": {
            "name": "Cormorant Garamond",
            "css": "Cormorant Garamond, Georgia, serif"
        },
        "body": {
            "name": "Nunito",
            "css": "Nunito, system-ui, sans-serif"
        }
    },
    "cssClasses": "variable-size-headings elegant-theme",
    "presets": {
        "light": {
            "text": {
                "h1": [
                    "#2d2d3a"
                ],
                "h2": [
                    "#2d2d3a"
                ],
                "h3": [
                    "#3d3d4a"
                ],
                "h4": [
                    "#3d3d4a"
                ],
                "body": "#4a4a5a",
                "header": "#7a7a8a",
                "footer": "#7a7a8a"
            },
            "backgrounds": {
                "general": {
                    "type": "solid",
                    "color": "#faf8f5"
                },
                "cover": {
                    "type": "gradient",
                    "colors": [
                        "#fce4ec",
                        "#f3e5f5"
                    ]
                },
                "title": {
                    "type": "solid",
                    "color": "#faf8f5"
                },
                "section": {
                    "type": "solid",
                    "color": "#2d2d3a"
                }
            },
            "accents": [
                "#d4a5a5",
                "#2d2d3a",
                "#b39ddb",
                "#90caf9",
                "#a5d6a7",
                "#ffcc80"
            ]
        },
        "dark": {
            "text": {
                "h1": [
                    "#faf8f5"
                ],
                "h2": [
                    "#faf8f5"
                ],
                "h3": [
                    "#e5e3e0"
                ],
                "h4": [
                    "#e5e3e0"
                ],
                "body": "#d5d3d0",
                "header": "#a5a3a0",
                "footer": "#a5a3a0"
            },
            "backgrounds": {
                "general": {
                    "type": "solid",
                    "color": "#2d2d3a"
                },
                "cover": {
                    "type": "gradient",
                    "colors": [
                        "#1a1a24",
                        "#2d2d3a"
                    ]
                },
                "title": {
                    "type": "solid",
                    "color": "#2d2d3a"
                },
                "section": {
                    "type": "solid",
                    "color": "#d4a5a5"
                }
            },
            "accents": [
                "#d4a5a5",
                "#faf8f5",
                "#b39ddb",
                "#90caf9",
                "#a5d6a7",
                "#ffcc80"
            ]
        }
    }
}