    "dark": {
      "background": "#0d1117",
      "titleText": "#e6e6e6",
      "bodyText": "#abb2bf",
      "frame": "#161b22",
      "accent1": "#ff6b9d",
      "accent2": "#c678dd",
      "accent3": "#61afef",
      "accent4": "#98c379",
      "accent5": "#e5c07b",
      "accent6": "#56b6c2"
