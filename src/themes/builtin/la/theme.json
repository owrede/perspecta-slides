{
    "name": "LA",
    "version": "2.0.0",
    "author": "Perspecta",
    "description": "Warm, vibrant theme inspired by Los Angeles sunsets",
    "fonts": {
        "title": {
            "name": "Poppins",
            "css": "Poppins, system-ui, sans-serif"
        },
        "body": {
            "name": "Poppins",
            "css": "Poppins, system-ui, sans-serif"
        }
    },
    "cssClasses": "variable-size-headings",
    "presets": {
        "light": {
            "text": {
                "h1": [
                    "#1a1a1a"
                ],
                "h2": [
                    "#1a1a1a"
                ],
                "h3": [
                    "#333333"
                ],
                "h4": [
                    "#333333"
                ],
                "body": "#444444",
                "header": "#666666",
                "footer": "#666666"
            },
            "backgrounds": {
                "general": {
                    "type": "dynamic",
                    "colors": [
                        "#ffecd2",
                        "#fcb69f",
                        "#ff9a9e",
                        "#fad0c4"
                    ]
                },
                "cover": {
                    "type": "gradient",
                    "colors": [
                        "#ff9a9e",
                        "#fad0c4"
                    ]
                },
                "title": {
                    "type": "solid",
                    "color": "#ffecd2"
                },
                "section": {
                    "type": "gradient",
                    "colors": [
                        "#ff6b6b",
                        "#ee5a5a"
                    ]
                }
            },
            "accents": [
                "#ff6b6b",
                "#ffa502",
                "#ff9ff3",
                "#48dbfb",
                "#1dd1a1",
                "#5f27cd"
            ]
        },
        "dark": {
            "text": {
                "h1": [
                    "#ffffff"
                ],
                "h2": [
                    "#ffffff"
                ],
                "h3": [
                    "#f0e6dc"
                ],
                "h4": [
                    "#f0e6dc"
                ],
                "body": "#e0d6cc",
                "header": "#a09691",
                "footer": "#a09691"
            },
            "backgrounds": {
                "general": {
                    "type": "dynamic",
                    "colors": [
                        "#2d1b2e",
                        "#4a2545",
                        "#6b2d5b",
                        "#8b3566"
                    ]
                },
                "cover": {
                    "type": "gradient",
                    "colors": [
                        "#2d1b2e",
                        "#4a2545"
                    ]
                },
                "title": {
                    "type": "solid",
                    "color": "#2d1b2e"
                },
                "section": {
                    "type": "solid",
                    "color": "#ff6b6b"
                }
            },
            "accents": [
                "#ff6b6b",
                "#ffa502",
                "#ff9ff3",
                "#48dbfb",
                "#1dd1a1",
                "#5f27cd"
            ]
        }
    }
}